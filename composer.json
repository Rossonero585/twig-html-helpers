{
    "name": "njh/twig-html-helpers",
    "description": "Rails style view helpers for HTML tags in PHP Twig",
    "keywords": ["twig","html","tag"],
    "homepage": "https://github.com/njh/twig-html-helpers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicholas Humfrey",
            "email": "njh@aelius.com",
            "homepage": "http://www.aelius.com/njh/"
        }
    ],
    "require": {
        "php": ">=5.2.4",
        "twig/twig": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.5.15"
    },
    "autoload": {
        "psr-4": { "Njh\\TwigHTMLHelpers\\": "lib/" }
    },
    "autoload-dev": {
        "psr-4" : { "Njh\\Tests\\" : "tests/" }
    }
}
